		<!-- if you want to test a specific level quickly without reordering, you can enter an index.  0 is the first, 1 is second, etc. -->
		<!-- <testingStartAtLevelIndex>14</testingStartAtLevelIndex> -->

		<!-- how to draw the line, other general properties -->
		<doFairyCollisions>never</doFairyCollisions> <!-- always / never / highperformance.  have petals collide off fairy -->
		<rotatePetals>never</rotatePetals> <!-- always / never / highperformance.  Anything else or omitted and defaults to always -->
		<hostFrameReset>120</hostFrameReset> <!-- when host mood/text changes, we reset to neutral after this many frames -->
		<debugLinePoints>false</debugLinePoints> <!-- adds a LOT of logging around loop points.  ONLY FOR DEBUGGING -->
		<spawnReplacementPetals>true</spawnReplacementPetals> <!-- spawn a replacement for each petal that is captured? -->
		<tracerColor>0xf4ea8b</tracerColor> <!-- color for the line -->
		<tracerBlurAmount>5</tracerBlurAmount> <!-- amount of blur effect to apply to line -->
		<tracerWidth>5</tracerWidth> <!-- width of the line -->
		<tracerAlpha>100</tracerAlpha> <!-- trqnsparency of the line -->
		<maxTracerDistance>800</maxTracerDistance> <!-- maximum length of line -->
		<loopDetectionThreshold>3.5</loopDetectionThreshold> <!-- default 1, bump it to 1.1, 1.2, etc. to make it a little easier to complete a loop -->
		<loopFillColorSuccess>0x25D339</loopFillColorSuccess> <!-- color for the fill of a completed successful loop -->
		<loopFillColorFail>0xDD3F1F</loopFillColorFail> <!-- color for the fill of a completed invalid loop (only one element for instance) -->
		<loopFillAlpha>0.6</loopFillAlpha> <!-- transparency of a completed loop -->
		<loopFillFadeRate>0.05</loopFillFadeRate> <!-- rate at which fill will fade out -->
		<tracerFadeRate>0.02</tracerFadeRate> <!-- rate at which line will fade out once it starts... -->
		<tracerFadeDelay>60</tracerFadeDelay> <!-- frames to wait before line will start to fade out once mouse is up -->
		<capturedItemFadeRate>0.04</capturedItemFadeRate> <!-- rate at which a captured item (petal, powerup) will fade out -->
		<tracerRechargePercentage>1</tracerRechargePercentage> <!-- % of tracer you want recharged each frame that drawing has stopped -->
		<captureRechargePercentage>0</captureRechargePercentage> <!-- % of tracer you want recharged as a reward for a successful capture -->
		<levelCompletionDelay>100</levelCompletionDelay> <!-- after objective is achieved, wait this many frames before popping up the congrats message -->
		<petalCaptureRequiredPercentage>50</petalCaptureRequiredPercentage> <!-- what percentage of a petal needs to be within the loop for us to count it as captured? -->
		<powerupCaptureRequiredPercentage>50</powerupCaptureRequiredPercentage> <!-- what percentage of a powerup needs to be within the loop for us to count it as captured? -->
		<pixieDustDropFrequency>0</pixieDustDropFrequency> <!-- how often to drop a pixie dust particle - 1 means on every drawn point, 2 means every other point, 3 is every 3rd point, etc.  Strike a balance between visual and performance.  0 means never -->
		<playerLives>3</playerLives> <!-- how many lives does the player get?  Use -1 for unlimited lives -->
		<bounceBufferPercentage>20</bounceBufferPercentage> <!-- portion of the screen on top/bottom to consider for bouncing (if a petal if floating offscreen, we reverse it to get it back in the middle of the play area -->
		<startWarningSound>5</startWarningSound> <!-- at this many seconds remaining, start the warning sound -->

		<!-- wind configuration -->
		<windDisplayVane>false</windDisplayVane> <!-- display the debug windvane or not -->
		<windRandomHeadingFluctuation>10</windRandomHeadingFluctuation> <!-- when getting a random heading from WindManager, it will be true wind +/- this amount -->
		<windChangeFrequency>300</windChangeFrequency> <!-- wind should change every N frames (randomly, not actual) -->

		<!-- scoring -->
		<standardPetalScore>10</standardPetalScore> <!-- points to get with each capture of a standard petal -->
		<sameColorPetalBonus>0</sameColorPetalBonus> <!-- if all petals in a capture are the same color, each is worth a little more -->
		<captureTimeBonus>0</captureTimeBonus> <!-- amount of time to add to timer with each capture -->
		<multiColorPunishment>0</multiColorPunishment> <!-- points to lose if you complete a loop with multiple colors -->
		<levelBonusDaysLeft>10</levelBonusDaysLeft> <!-- points to multiply by days remaining as an end-of-level bonus -->
		<powerupScore>10</powerupScore> <!-- points you received for looping a powerup -->
		<!-- <bonusLifeEveryNLevels>0</bonusLifeEveryNLevels> --> <!-- give an extra life each time they complete N non-bonus levels.  0 disables.  -->
		<chainMultiplier>20</chainMultiplier> <!-- chain length times this value gives a bonus -->
		<biggestCaptureMultiplier>5</biggestCaptureMultiplier> <!-- size of biggest capture times this value gives a bonus -->
		
		<!-- bonus petals currently turned off... -->
		<bonusPetalScore>150</bonusPetalScore> <!-- points to get with each capture of a petal from bonus flower -->
		<bonusFlowerScore>1000</bonusFlowerScore> <!-- points to get with a successful completion of the bonus flower -->

		<!-- powerups -->
		<powerupDuration>5</powerupDuration> <!-- how many seconds does a default powerup last -->
		<timeBonusAmount>10</timeBonusAmount> <!-- if you get the "extra time" powerup - how much extra time is it worth? -->

		<!-- defaults that can be overridden in individual level configuration to scale difficulty -->
		<minPetalSpeed>1</minPetalSpeed>
		<maxPetalSpeed>3</maxPetalSpeed>
		<timeLimit>30</timeLimit>
		<minLegalPetalCount>1</minLegalPetalCount>
		<objectiveTitle>Objective</objectiveTitle>

		<!-- particle manager -->
