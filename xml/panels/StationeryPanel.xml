		<startedText target="dimClip.getStarted">Get Started!</startedText>
		<switchFairyBtn target="dimClip.switchFairy.caption">Change Fairy</switchFairyBtn>
		<preview1 target="dimClip.stationerySelector.preview_btn.caption">Preview</preview1>
		<preview2 target="dimClip.cardSelector.preview_btn.caption">Preview</preview2>
		<printBtn target="confirm.printBtn.caption">Print</printBtn>
		<cancelBtn target="confirm.cancelBtn.caption">Cancel</cancelBtn>
	</copy>
	<config>
		<sheet>css/loginformat.css</sheet>
		<stationery>
			<name>s1</name>
			<name>s2</name>
			<name>s3</name>
			<name>s4</name>
			<tb>stb1</tb>
			<tb>stb2</tb>
			<tb>stb3</tb>
			<tb>stb4</tb>
		</stationery>
		<card>
			<name>c1</name>
			<name>c2</name>
			<name>c3</name>
			<name>c4</name>
			<tb>ctb1</tb>
			<tb>ctb2</tb>
			<tb>ctb3</tb>
			<tb>ctb4</tb>
		</card>
