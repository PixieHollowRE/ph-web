		<inContextAlerts>
			<!-- node names (inContextDecorating, inContextShops, etc.) correspond to id's in tutorialAssets.xml & consts in TutorialManager.as -->
			<inContextDecorating>
				<title>Decorate Your Fairy Home!</title>
				<description>You can decorate your home with furniture found in your storage chest.</description>
				<!-- <highlightData>-590,-385,25</highlightData>  --> <!-- either an x/y + radius, or a special case that InContextAlert.as handles -->
				<highlightData>sparkelsandarrow,-590,-385,25,-485,-280,300</highlightData> <!-- arrow case means x/y coord plus rotation for the arrow -->
				<featureIcon>chest</featureIcon> <!-- corresponds to a frame of In Context Alert Feature Icon in notification.fla -->
				<tutorialToOpen>decorating</tutorialToOpen> <!-- corresponds to [tutorial id="x"] in tutorialAssets.xml -->
				<leafjournalPage>decorating</leafjournalPage>	
			</inContextDecorating>
			<inContextShops>
				<title>Buy new items in our shops!</title>
				<description>Dress up in new fashions or decorate your home with new items!</description>
				<highlightData>shopkeeper,-420,-255,-120,-255,-120,-195,-420,-195</highlightData> <!-- special case, set of points to follow -->
				<featureIcon>shopkeeper</featureIcon>
				<tutorialToOpen>shops</tutorialToOpen>
				<leafjournalPage>shops</leafjournalPage>
			</inContextShops>
			<inContextParties>
				<title>Have a Pixie Party!</title>
				<description>In this shop, you can buy games and decorations to throw your own Pixie Party!</description>
				<highlightData>shopkeeper,-420,-255,-120,-255,-120,-195,-420,-195</highlightData> <!-- special case, set of points to follow -->
				<featureIcon>shopkeeper</featureIcon>
				<tutorialToOpen>parties</tutorialToOpen>
				<leafjournalPage>parties</leafjournalPage>
			</inContextParties>

			<inContextBadges>
				<title>You've earned your first badge!</title>
				<description>Badges are awarded to celebrate all the great things you do in Pixie Hollow!</description>
				<!--<highlightData>-647,-360,30</highlightData>-->
				<highlightData>sparkelsandarrow,-647,-360,30,-530,-250,300</highlightData> <!-- arrow case means x/y coord plus rotation for the arrow -->
				<featureIcon>leafJournal</featureIcon>
				<tutorialToOpen>badges</tutorialToOpen>
				<leafjournalPage>badges</leafjournalPage>
			</inContextBadges>

			<inContextGathering>
				<title>You found an ingredient!</title>
				<description>Ingredients can be used all over Pixie Hollow for shopping and crafting!</description>
				<highlightData>sparkelsandarrow,-686,-320,25,-580,-210,300</highlightData>
				<featureIcon>pouch</featureIcon>
				<tutorialToOpen>gathering</tutorialToOpen>
				<leafjournalPage>gathering</leafjournalPage>
			</inContextGathering>

			<inContextNewHome>
				<title>Move into a new Fairy Home!</title>
