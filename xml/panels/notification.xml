				<highlightData>shopkeeper,-420,-255,-120,-255,-120,-195,-420,-195</highlightData>
				<featureIcon>blank</featureIcon>
				<tutorialToOpen>fairygardens</tutorialToOpen>
				<leafjournalPage></leafjournalPage>
				<alternateCornerIconFrame>garden</alternateCornerIconFrame>
