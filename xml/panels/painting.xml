		<!-- Game Constants  -->
		<canvas_x>200</canvas_x>                 <!-- where the canvas is on the stage -->
		<canvas_y>23</canvas_y>        
		<translucent_per>.5</translucent_per>    <!-- alpha value for mixing colors -->
		<blink_delay>1500</blink_delay>          <!-- how long the UI blink animations take in milliseconds -->
		<red_x_timer>500</red_x_timer>           <!-- how long the mistake markers stay visible -->
		<show_red_x>true</show_red_x>            <!-- could be used to disable mistake markers (type anything except 'true') -->

	</copy>
	<config>
	
	<colorPalette>3</colorPalette> <!-- colorPalette has nothing to do with the gameplay (this line appears in every game) -->

	<!-- Default paint colors -->
	<colors>
		<color red="0xF9" green="0x0F" blue="0x0F"></color> <!-- 0 red,     always -->
		<color red="0xFF" green="0xE8" blue="0x1A"></color> <!-- 1 yellow,  always -->
		<color red="0x36" green="0x63" blue="0xB4"></color> <!-- 2 blue,    always -->
		<color red="0xBC" green="0xDE" blue="0xFE"></color> <!-- 3 special, changes per level -->
		<color red="0xFF" green="0xFF" blue="0xFF"></color> <!-- 4 white,   unpainted segments -->
		<color red="0x00" green="0x00" blue="0x00"></color> <!-- 5 black,   used for mistakes / ruined segments -->
		<color red="0xEE" green="0x8E" blue="0x00"></color> <!-- 6 orange   -->
		<color red="0x30" green="0xB6" blue="0x30"></color> <!-- 7 green    -->
		<color red="0xB0" green="0x04" blue="0xFB"></color> <!-- 8 purple   -->
		<color red="0x90" green="0x5D" blue="0x2C"></color> <!-- 9 brown,   triple combination -->
	</colors>
	
