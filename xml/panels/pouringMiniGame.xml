	<config>

		<colorPalette>3</colorPalette>
		<batterRate>0.74</batterRate>
		<timerLeakRate>350</timerLeakRate>
		<perfectPourBonus>5</perfectPourBonus>

		<!--
			perTapMinusScore should be configured as a positive value. 

			This value gets subtracted from the final score for each tab. 
			Making this value +ve will add to the score instead of subtracting (which we dont want).
		-->
		<perTapMinusScore>5</perTapMinusScore>

		<!--
